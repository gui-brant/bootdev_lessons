function getMonthlyPrice(tier) {
  switch (tier) {
    case "basic":
      return 10000;
    case "premium":
      return 15000;
    case "enterprise":
      return 50000;
    default:
      return 0;
  }
     
  return tier*100
}

// don't touch below this line

export { getMonthlyPrice };

//This language does not execute code from start to finish, compiler style. It looks ahaed, hence you can call functions before their definition. That's called hoisting a function.
