class Trie:
    def exists(self, word):
        current = self.root
        for char in word:
            if char not in current:
                return False
            current = current[char]
        print(current)
        if self.end_symbol in current:#if current == self.end_symbol: --> you're working with dicts, so that's checked by a dictionary membership test like the one shown!
            return True
        else:
            return False
                

    # don't touch below this line

    def add(self, word):
        current = self.root
        for letter in word:
            if letter not in current:
                current[letter] = {}
            current = current[letter]
        current[self.end_symbol] = True

    def __init__(self):
        self.root = {}
        self.end_symbol = "*"

