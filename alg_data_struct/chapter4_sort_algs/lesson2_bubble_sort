def bubble_sort(nums):
    swapping = True
    end = len(nums)
    forward_value = 0 #You don't need two temp values
    backward_value = 0 #You don't need to declarate temp values here
    while swapping == True: #while swapping == True is the same as while swapping:
        swapping = False
        for i in range(1, end):
            if nums[i-1] > nums[i]:
                backward_value = nums[i-1] #temp = nums[i-1]
                forward_value = nums[i] #nums[i-1] = nums[i]
                nums[i] = backward_value #nums[i] = temp
                nums[i-1] = forward_value
                swapping = True
        end -= 1
    return nums
